<ul>
  <% @notes.each do |note| %>
    <li><%= note.entry %>
      <%= link_to image_tag('trash-can-icon.jpg', :size => '20x20'), note_url(note.id), :method => 'delete' %>
      <%= link_to image_tag('pencil.png', :size => '20x20'), edit_note_url(note.id) %>
    </li>
  <% end %>
  <%= form_for Note.new do |f| %>
    <%= f.text_field :entry, :size => '50' %>
  <% end %>
</ul>
