<ul>
  <% @notes.each do |note| %>
    <li id="note_<%= note.id %>"><%= note.entry %>
      <%= link_to image_tag('trash-can-icon.jpg', :size => '20x20'), note, :method => 'delete', :remote => true %>
      <%= link_to image_tag('pencil.png', :size => '20x20'), edit_note_url(note) %>
    </li>
  <% end %>
  <%= form_for Note.new do |f| %>
    <%= f.text_field :entry, :size => '50' %>
  <% end %>
</ul>
